<?xml version="1.0" encoding="utf-8" ?>
<Defs>

   <RecipeDef>
      <defName>PrepairMeat</defName>
      <label>Prepare meat</label>
      <description>Cut off excess fat and trim the meat.</description>
      <jobString>Preparing meat.</jobString>
      <workAmount>200</workAmount>
      <workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
      <effectWorking>ButcherFlesh</effectWorking>
      <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
      <allowMixingIngredients>true</allowMixingIngredients>
      <ingredients>
         <li>
            <filter>
               <categories>
                  <li>MeatRaw</li>
               </categories>
            </filter>
            <count>25</count>
         </li>
      </ingredients>
      <products>
         <SCMeat>25</SCMeat>
      </products>
      <fixedIngredientFilter>
         <categories>
            <li>MeatRaw</li>
         </categories>
         <disallowedThingDefs>
            <li>SCMeat</li>
         </disallowedThingDefs>
      </fixedIngredientFilter>
      <defaultIngredientFilter>
         <categories>
            <li>Foods</li>
            <li>Manufactured</li>
         </categories>
      </defaultIngredientFilter>
      <workSkill>Cooking</workSkill>
      <recipeUsers>
         <li>TableButcher</li>
      </recipeUsers>
   </RecipeDef>

</Defs>