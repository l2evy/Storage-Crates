<Defs>



   <!-- =======Minerals======= -->
   <RecipeDef>
      <defName>unpackSteelCrate</defName>
      <label>Un-pack Steel Crate (75)</label>
      <description>Un-Pack 75 steel pieces from a crate.</description>
      <jobString>Packing heat.</jobString>
      <workAmount>100</workAmount>
      <workSpeedStat>PackingTime</workSpeedStat>
      <effectWorking>ConstructMetal</effectWorking>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>SteelCrate</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <products>
         <Steel>75</Steel>
         <KMSteelPlate>6</KMSteelPlate>
      </products>
      <fixedIngredientFilter>
         <thingDefs>
            <li>SteelCrate</li>
         </thingDefs>
      </fixedIngredientFilter>
      <recipeUsers>
         <li>UnpackingBenchTierOne</li>
      </recipeUsers>
   </RecipeDef>

