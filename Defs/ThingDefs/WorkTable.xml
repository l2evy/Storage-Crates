<?xml version="1.0" encoding="utf-8" ?>
<Defs>

   <ThingDef Name="BuildingBase" Abstract="True">
      <category>Building</category>
      <thingClass>Building</thingClass>
      <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
      <selectable>true</selectable>
      <drawerType>MapMeshAndRealTime</drawerType>
      <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
      <repairEffect>Repair</repairEffect>
      <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
      <filthLeaving>Filth_RubbleBuilding</filthLeaving>
      <minifiedDef>MinifiedThing</minifiedDef>
   </ThingDef>


   <ThingDef Name="PackingBenchTierOne" ParentName="BuildingBase">
      <defName>PackingBenchTierOne</defName>
      <label>Packing Bench</label>
      <thingClass>KompressionMod.PackingBenchTierOne</thingClass>
      <description>A workbench where you can cut your meal with plasma torch or pack a crate.</description>
      <graphicData>
         <texPath>Things/Building/Frames/PackingBenchTier1_Open</texPath>
         <shaderType>CutoutComplex</shaderType>
         <graphicClass>Graphic_Single</graphicClass>
         <drawSize>(3,1)</drawSize>
         <damageData>
          <cornerTL>Damage/Corner</cornerTL>
          <cornerTR>Damage/Corner</cornerTR>
          <cornerBL>Damage/Corner</cornerBL>
          <cornerBR>Damage/Corner</cornerBR>
         </damageData>
      </graphicData>
      <costList>
         <KMSteelPlate>6</KMSteelPlate>
      </costList>
      <stuffCategories>
         <li>Metallic</li>
         <li>Woody</li>
      </stuffCategories>
      <costStuffCount>30</costStuffCount>
      <altitudeLayer>Building</altitudeLayer>
      <statBases>
         <WorkToMake>1500</WorkToMake>
         <MaxHitPoints>250</MaxHitPoints>
         <Flammability>0.5</Flammability>
         <Mass>22</Mass>
      </statBases>
      <minifiedDef />
      <size>(3,1)</size>
      <tickerType>Normal</tickerType>
      <designationCategory>Production</designationCategory>
      <passability>Impassable</passability>
      <hasInteractionCell>True</hasInteractionCell>
      <interactionCellOffset>(0,0,-1)</interactionCellOffset>
      <surfaceType>Item</surfaceType>
      <inspectorTabs>
         <li>ITab_Bills</li>
      </inspectorTabs>
      <comps>
         <li Class="CompProperties_Power">
            <compClass>CompPowerTrader</compClass>
            <shortCircuitInRain>true</shortCircuitInRain>
            <basePowerConsumption>350</basePowerConsumption>
            <soundPowerOn>Power_OnSmall</soundPowerOn>
            <soundPowerOff>Power_OffSmall</soundPowerOff>
         </li>
         <li Class="CompProperties_AffectedByFacilities">
            <linkableFacilities>
               <li>ToolCabinet</li>
            </linkableFacilities>
         </li>
         <li Class="CompProperties_Flickable" />
         <li Class="CompProperties_Breakdownable" />
      </comps>
   </ThingDef>

